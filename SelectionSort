def selection_sort(lista):
    n = len(lista)

    for i in range(n):
        # Assume que o menor elemento é o atual
        min_index = i

        # Procura o menor elemento no restante da lista
        for j in range(i + 1, n):
            if lista[j] < lista[min_index]:
                min_index = j

        # Troca o menor elemento encontrado com o da posição atual
        lista[i], lista[min_index] = lista[min_index], lista[i]

    return lista

# Exemplo de uso
nums = [64, 25, 12, 22, 11]
ordenada = selection_sort(nums)
print(ordenada)
